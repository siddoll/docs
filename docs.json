{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Typelense",
  "colors": {
    "primary": "#4D43FE",
    "light": "#4D43FE",
    "dark": "#4D43FE"
  },
  "favicon": "/typelense_favicon_white.svg",
  "modeToggle": {
    "default": "light",
    "isHidden": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "plansandcredits",
              "faq",
              "getting-started"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Announcements",
          "href": "https://typelense.io/",
          "icon": "newspaper"
        },
        {
          "anchor": "Status",
          "href": "https://status.typelense.io/",
          "icon": "signal"
        },
        {
          "anchor": "Feature Requests",
          "href": "https://app.typelense.io/feature-requests",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/typelense_logo.svg",
    "dark": "/logo/typelense_logo_white.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@typelense.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.typelense.io",
      "color": "#4D43FE"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/typelense"
    }
  }
}